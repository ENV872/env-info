source 'https://rubygems.org'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'github-pages', versions['github-pages']

gem 'jekyll-last-modified-at', :require => 'jekyll-last-modified-at'

gem 'jemoji'
gem 'jekyll-gist'
gem 'jekyll-sitemap'